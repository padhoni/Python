day=2

if day==1:
    print("Monday")
elif day ==2:
    print("Tuesday")
elif day ==3:
    print("Wednesday")
elif day ==4:
    print("Thrusday")
elif day ==5:
    print("Friday")
elif day ==6:
    print("Saturday")
else:
    print("Sunday")
